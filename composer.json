{
    "name": "logipro/drivemanager",
    "type": "app",
    "description": "start a new PHP project with a simple base structure",
    "keywords": ["php", "DDT"],
    "homepage": "https://github.com/logipro/drivemanager",
    "license": "Copyrighted",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Frédéric Royet",
            "email": "frederic.royet@logipro.com",
            "homepage": "https://www.linkedin.com/in/frederic-royet-42568566/",
            "role": "Project Founder"
        }
    ],
    "require": {
        "php": ">=8.3",
        "symfony/framework-bundle": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/browser-kit": "^7.0",
        "symfony/orm-pack": "^2.4",
        "symfony/http-foundation": "^7.0",
        "nelmio/api-doc-bundle": "^4.26",
        "nelmio/cors-bundle": "^2.4",
        "symfony/dotenv": "^7.1",
        "vlucas/phpdotenv": "^5.6",
        "symfony/dependency-injection": "^7.1",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/orm": "^3.2"
    },
    "require-dev": {
        "infection/extension-installer": "0.1.2",
        "infection/infection": "^0.28",
        "phpstan/phpdoc-parser": "^1.28",
        "phpstan/phpstan": "1.10",
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "3.*",
        "behat/behat": "^3.14",
        "mikey179/vfsstream": "^1.6",
        "symfony/http-client": "^7.0",
        "phariscope/doctrine-testing-tools": "^1.3",
        "symfony/test-pack": "^1.0"
    },
    "autoload": {
        "psr-4" : {
            "DriveManager\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "DriveManager\\Tests\\" : "tests/unit"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "runtime": {
            "dotenv_path": "src/Infrastructure/Shared/Symfony/.env"
        }
    }
}
